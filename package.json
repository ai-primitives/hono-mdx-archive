{
  "name": "hono-mdx",
  "version": "0.1.0",
  "description": "MDX renderer using Hono's built-in JSX renderer with Suspense and streaming support",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "keywords": [
    "mdx",
    "hono",
    "jsx",
    "cloudflare",
    "workers"
  ],
  "author": "AI Primitives",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "esbuild-wasm": "^0.24.0",
    "hono": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
