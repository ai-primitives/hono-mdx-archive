{
  "name": "hono-mdx",
  "version": "0.1.0",
  "description": "MDX renderer using Hono's built-in JSX renderer with Suspense and streaming support",
  "homepage": "https://mdx.org.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/hono-mdx.git"
  },
  "bugs": {
    "url": "https://github.com/ai-primitives/hono-mdx/issues"
  },
  "type": "module",
  "scripts": {
    "build": "vite build && node scripts/build.js",
    "build:worker": "node scripts/build.js",
    "dev": "wrangler dev src/worker.ts",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "keywords": [
    "mdx",
    "hono",
    "jsx",
    "cloudflare",
    "workers"
  ],
  "author": "AI Primitives",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@monaco-editor/react": "^4.6.0",
    "@picocss/pico": "^2.0.6",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/mdx": "^2.0.13",
    "@types/mdx-js__react": "^2.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.16",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "happy-dom": "13",
    "jsdom": "^24.1.3",
    "postcss": "^8.4.49",
    "prettier": "^3.0.0",
    "rehype-highlight": "^7.0.1",
    "rehype-prism": "^2.3.3",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "6",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^2.1.8",
    "wrangler": "^3.95.0"
  },
  "dependencies": {
    "@clickhouse/client-web": "^1.9.1",
    "@openauthjs/openauth": "^0.2.0",
    "@standard-schema/spec": "^1.0.0-beta.4",
    "esbuild-wasm": "^0.24.0",
    "hono": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "valibot": "^0.24.1"
  }
}
